{
    "manifest_version": 3,
    "name": "WizarMark",
    "version": "1.0",
    "description": "A simple extension to bookmark websites",
    "permissions": [
        "activeTab",
        "tabs",
        "bookmarks",
        "sidePanel"
    ],
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "action": {
        "default_popup": "index.html"
    }
}